	private List<Comic> estadoActivo(List<Comic)listaComics){
		
		List<cm> estadoActivo = new ArrayList<>();
		
		for (cm cm : estadoActivo) {
			if (cm.getEstado()==(EstadoEnum.Activo)) {
				estadoActivo.add(cm);
			}
		}
	}
				
			
			private list<Comic> estadoInactivo(List<Comic)listaComics){
				
				List<cm> estadoInactivo = new ArrayList<>();
				
				for (cm cm : estadoInactivo) {
					if (cm.getEstado()==(EstadoEnum.Inactivo)) {
						estadoInactivo.add(cm);
		}
			}
		
	}
	
	


