package com.hbt.semillero.enums;

public enum EstadoComicEnum {
	
	/**
	 * Codigo activo
	 */
	ACTIVO("codigo.comic.activo"),
	
	/**
	 * Codigo inactivo
	 */
	INACTIVO("codigo.comic.inactivo");
	
	private String codigoMensaje;
	
	EstadoComicEnum(String codigoMensaje) {
		this.codigoMensaje = codigoMensaje;
	}

	/**
	 * @return el codigoMensaje
	 */
	public String getCodigoMensaje() {
		return codigoMensaje;
	}

	/**
	 * @param codigoMensaje el codigoMensaje a establecer
	 */
	public void setCodigoMensaje(String codigoMensaje) {
		this.codigoMensaje = codigoMensaje;
	}

}
